{
	"rewrites": [
		{ "source": "/api/feedback", "destination": "/api/feedback.js" },
		{ "source": "/api/feedback/:id", "destination": "/api/feedback/[id].js" },
		{ "source": "/(.*)", "destination": "/index.html" }
	],
	"headers": [
		{
			"source": "/(.*)\\.js$",
			"headers": [{ "key": "Content-Type", "value": "application/javascript" }]
		},
		{
			"source": "/(.*)\\.jsx$",
			"headers": [{ "key": "Content-Type", "value": "application/javascript" }]
		}
	]
}
