{
	"routes": [
		{ "src": "/api/feedback", "dest": "/api/feedback.js" },
		{ "src": "/(.*)", "dest": "/index.html" }
	],
	"headers": [
		{
			"source": "/(.*)\\.js$",
			"headers": [{ "key": "Content-Type", "value": "application/javascript" }]
		},
		{
			"source": "/(.*)\\.jsx$",
			"headers": [{ "key": "Content-Type", "value": "application/javascript" }]
		}
	]
} 
